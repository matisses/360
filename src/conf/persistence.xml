    <class>co.matisses.persistence.web.entity.DetalleVentaPOS</class>
    <class>co.matisses.persistence.web.entity.OperacionCaja</class>
    <class>co.matisses.persistence.web.entity.PagoVentaPOS</class>
    <class>co.matisses.persistence.web.entity.EmpaqueVenta</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
