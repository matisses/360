    <properties/>
  </persistence-unit>
  <persistence-unit name="BARUWEBPU" transaction-type="JTA">
    <jta-data-source>java:/baruweb_ds</jta-data-source>
    <class>co.matisses.persistence.web.entity.ClienteWeb</class>
    <class>co.matisses.persistence.web.entity.DireccionClienteWeb</class>
